{
  "name": "go-starter-kit",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx src",
    "flow": "./node_modules/.bin/flow",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql  --extensions js jsx",
    "start": "node_modules/.bin/webpack-dev-server --mode development --watch --progress --devtool inline-source-map --content-base src/static",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p --config ./webpack.config.js --optimize-minimize"
  },
  "author": "olebedev",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "~>6.7.7",
    "babel-core": "~>6.24.1",
    "babel-eslint": "~>8.2.2",
    "babel-loader": "~>7.1.4",
    "babel-plugin-react-css-modules": "~>3.4.0",
    "babel-polyfill": "~>6.26.0",
    "babel-preset-env": "~>1.6.1",
    "babel-preset-react": "~>6.24.1",
    "babel-preset-stage-0": "~>6.24.1",
    "chroma-js": "~>1.4.0",
    "classnames": "~>2.2.5",
    "css-loader": "~>0.28.0",
    "cssnano": "~>4.1.7",
    "eslint": "~>4.19.1",
    "eslint-plugin-babel": "~>5.0.0",
    "eslint-plugin-flowtype": "~>2.46.1",
    "eslint-plugin-react": "~>7.7.0",
    "expose-loader": "~>0.7.1",
    "express": "~>4.16.3",
    "farce": "~>0.2.6",
    "file-loader": "~>0.11.1",
    "found": "~>0.3.14",
    "found-relay": "~>0.3.1",
    "graphql-import": "~>0.7.1",
    "graphql-tools": "~>4.0.3",
    "immutability-helper": "~>2.8.1",
    "is-url-external": "~>1.0.3",
    "mini-css-extract-plugin": "~>0.4.4",
    "node-sass": "~>4.9.0",
    "normalize.css": "~>6.0.0",
    "postcss-loader": "~>2.1.6",
    "precss": "~>1.4.0",
    "primer": "~>10.10.2",
    "proxy-middleware": "~>0.15.0",
    "ramda": "~>0.25.0",
    "react": "~>16",
    "react-component-octicons": "~>1.8.0",
    "react-dom": "~>16",
    "react-relay": "~>1.7.0",
    "react-select": "2",
    "relay-runtime": "~>1.6.0",
    "sass-loader": "~>7.0.1",
    "serialize-javascript": "~>1.5.0",
    "style-loader": "~>0.21.0",
    "url-loader": "~>0.5.8",
    "uuid": "~>3.2.1",
    "webpack": "4",
    "webpack-dev-middleware": "~>3.1.3",
    "webpack-hot-middleware": "~>2.18.0",
    "when": "~>3.7.8",
    "winston": "~>2.4.2"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0",
      "flow"
    ],
    "plugins": [
      [
        "relay",
        {
          "schema": "./schema.graphql"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "never"
      ],
      "no-unreachable": "error",
      "no-unexpected-multiline": "error",
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "multiline-comment-style": [
        "error",
        "separate-lines"
      ],
      "function-paren-newline": [
        "error",
        "consistent"
      ],
      "curly": [
        "error",
        "multi",
        "consistent"
      ]
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jest",
      "flowtype"
    ],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      },
      "import/resolver": {
        "node": {
          "paths": [
            "client"
          ],
          "extensions": [
            "",
            ".js",
            ".jsx",
            ".scss",
            ".css"
          ]
        }
      }
    }
  },
  "main": "index.js",
  "repository": "git@github.com:olebedev/go-starter-kit.git",
  "devDependencies": {
    "babel-cli": "~>6.26.0",
    "babel-jest": "~>22.4.3",
    "babel-plugin-module-resolver": "~>3.1.1",
    "babel-plugin-relay": "~>1.5.0",
    "babel-preset-flow": "~>6.23.0",
    "enzyme": "~>3.8.0",
    "enzyme-adapter-react-16": "~>1.7.1",
    "eslint-config-airbnb": "~>16.1.0",
    "eslint-import-resolver-babel-module": "~>4.0.0",
    "eslint-import-resolver-webpack": "~>0.10.1",
    "eslint-plugin-import": "~>2.14.0",
    "eslint-plugin-jest": "~>21.15.0",
    "eslint-plugin-jsx-a11y": "~>6.0.3",
    "flow-bin": "~>0.69.0",
    "graphql": "~>0.13.2",
    "identity-obj-proxy": "~>3.0.0",
    "jest": "~>22.4.3",
    "jest-enzyme": "~>7.0.1",
    "react-test-renderer": "16",
    "relay-compiler": "~>1.6.0",
    "semver": "~>5.6.0",
    "webpack-cli": "~>2.0.15",
    "webpack-dev-server": "~>3.1.10",
    "webpack-manifest-plugin": "~>2.0.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/src/__mock__/jestSetup.js"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
