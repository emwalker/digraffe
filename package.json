{
  "name": "go-starter-kit",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx src",
    "flow": "./node_modules/.bin/flow",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql  --extensions js jsx",
    "start": "node_modules/.bin/webpack-dev-server --mode development --watch --progress --devtool inline-source-map --content-base src/static",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p --config ./webpack.config.js --optimize-minimize"
  },
  "author": "olebedev",
  "license": "MIT",
  "dependencies": {
    "@primer/css": "^12.4.1-rc.e761a3e",
    "array-move": "^2.1.0",
    "autoprefixer": "~9.6.0",
    "chroma-js": "~2.0.3",
    "classnames": "~>2.2.5",
    "core-js": "^2.6.9",
    "css-loader": "~3.0.0",
    "es6-promise-debounce": "^1.0.1",
    "expose-loader": "~>0.7.1",
    "express": "~4.17.1",
    "farce": "~>0.2.6",
    "file-loader": "~4.0.0",
    "found": "~0.4.0",
    "found-relay": "^0.4.0",
    "graphql-import": "~>0.7.1",
    "graphql-tools": "~4.0.4",
    "immutability-helper": "~3.0.1",
    "is-url-external": "~>1.0.3",
    "mini-css-extract-plugin": "~0.7.0",
    "node-sass": "~4.12.0",
    "normalize.css": "~8.0.1",
    "postcss-loader": "~3.0.0",
    "precss": "~4.0.0",
    "proxy-middleware": "~>0.15.0",
    "ramda": "~0.26.1",
    "react": "~16.8.6",
    "react-document-title": "^2.0.3",
    "react-dom": "~16.8.6",
    "react-force-graph": "^1.23.1",
    "react-google-recaptcha": "^1.0.5",
    "react-icons": "^3.7.0",
    "react-markdown": "^4.0.8",
    "react-relay": "~5.0.0",
    "react-responsive": "^7.0.0",
    "react-select": "3.0.4",
    "react-social-login-buttons": "^2.3.1",
    "react-sortable-hoc": "^1.9.1",
    "react-timeago": "^4.4.0",
    "regenerator-runtime": "^0.13.2",
    "relay-runtime": "~5.0.0",
    "sass-loader": "~7.1.0",
    "serialize-javascript": "~1.7.0",
    "style-loader": "~0.23.1",
    "three": "^0.105.2",
    "url-loader": "~2.0.0",
    "uuid": "~3.3.2",
    "webpack": "^4.34.0",
    "webpack-dev-middleware": "~3.7.0",
    "webpack-hot-middleware": "~2.25.0",
    "when": "~>3.7.8",
    "winston": "~3.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-flow"
    ],
    "plugins": [
      [
        "relay",
        {
          "schema": "./schema.graphql"
        }
      ],
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-function-bind"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jest",
      "flowtype"
    ],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      },
      "import/resolver": {
        "node": {
          "paths": [
            "client"
          ],
          "extensions": [
            "",
            ".js",
            ".jsx",
            ".scss",
            ".css"
          ]
        }
      }
    }
  },
  "main": "index.js",
  "repository": "git@github.com:olebedev/go-starter-kit.git",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/traverse": "^7.4.5",
    "@babel/types": "^7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "~3.2.0",
    "babel-plugin-react-css-modules": "~5.2.6",
    "babel-plugin-relay": "^5.0.0",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.14.0",
    "eslint": "~5.16.0",
    "eslint-config-airbnb": "~17.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-import-resolver-webpack": "~0.11.1",
    "eslint-plugin-babel": "~5.3.0",
    "eslint-plugin-flowtype": "~3.10.3",
    "eslint-plugin-import": "~2.17.3",
    "eslint-plugin-jest": "~22.6.4",
    "eslint-plugin-jsx-a11y": "~6.2.1",
    "eslint-plugin-react": "~7.13.0",
    "flow-bin": "~0.101.0",
    "graphql": "~14.3.1",
    "identity-obj-proxy": "~>3.0.0",
    "jest": "~24.8.0",
    "jest-enzyme": "~7.0.2",
    "react-test-renderer": "16.8.6",
    "relay-compiler": "~5.0.0",
    "semver": "~6.1.1",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "~3.7.1",
    "webpack-manifest-plugin": "~>2.0.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/src/__mock__/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost/"
  },
  "peerDependencies": {
    "prop-types": "^15.5.7"
  }
}
