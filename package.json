{
  "name": "go-starter-kit",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "eslint": "eslint --ext .js --ext .jsx client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "olebedev",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "expose-loader": "^0.7.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "normalize.css": "^6.0.0",
    "postcss-functions": "^2.1.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "proxy-middleware": "^0.15.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-redux": "^4.4.8",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.1",
    "style-loader": "^0.16.1",
    "svgo-loader": "^1.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "whatwg-fetch": "^2.0.3",
    "when": "^3.7.8"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "dev": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "never"
      ],
      "no-unreachable": "error",
      "no-unexpected-multiline": "error",
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "multiline-comment-style": [
        "error",
        "separate-lines"
      ],
      "function-paren-newline": [
        "error",
        "consistent"
      ]
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "client"
          ],
          "extensions": [
            "",
            ".js",
            ".jsx",
            ".scss",
            ".css"
          ]
        }
      }
    }
  },
  "main": "index.js",
  "repository": "git@github.com:olebedev/go-starter-kit.git",
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "svgo": "^1.0.5"
  }
}
